{
  "manifest_version": 3,
  "name": "GMU RMP Scores",
  "version": "1.0.0",
  "author": "danny.ink",
  "description": "Adds RateMyProfessor scores to MyPatriotWeb",
  "icons": {
    "128": "icons/icon128.png"
  },
  "background": {
    "service_worker": "lib/background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "https://ssbstureg.gmu.edu/*"
      ],
      "js": [
        "lib/content.js"
      ],
      "run_at": "document_end"
    }
  ]
}
